{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "*.json",
      "use": "@vercel/static",
      "config": {
        "excludeFiles": ["schema.json", "package*.json"]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/submit",
      "dest": "/api/index.py"
    },
    {
      "src": "/analyze",
      "dest": "/api/index.py"
    },
    {
      "src": "/admin",
      "dest": "/admin.html"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/script.js",
      "dest": "/script.js"
    }
  ]
}
